{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "tiffinmate-monolith",
  "version": "1.0.0",
  "description": "TiffinMate Monolithic Backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:menu": "node scripts/test-menu-api.js",
    "test:api": "jest --config ./tests/jest.config.js",
    "test:emails": "node scripts/test-all-email-templates.js",
    "email:dev": "email dev --dir src/modules/email/templates/emails",
    "email:export": "email export --dir src/modules/email/templates/emails",
    "vercel-build": "nest build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.4.17",
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@react-email/tailwind": "^1.2.2",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "@vercel/analytics": "^1.5.0",
    "bcrypt": "^5.0.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.41.3",
    "expo-secure-store": "^15.0.7",
    "expo-server-sdk": "^4.0.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.1",
    "mongodb": "^5.9.2",
    "mongoose": "^7.0.0",
    "node-mailjet": "^6.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.15.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-email": "^4.3.1",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "resend": "^6.1.3",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@react-email/preview-server": "^4.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.0",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/node": "^3.0.0",
    "axios": "^1.8.4",
    "chalk": "^5.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*(spec|test)\\.(t|j)s$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
