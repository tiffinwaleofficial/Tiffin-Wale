{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "tiffinmate-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.0.0",
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 EXPO_NO_DOCTOR=1 bun exec expo start --clear",
    "start": "EXPO_NO_DOCTOR=1 bun exec expo start --clear",
    "build:web": "bun exec expo export --platform web",
    "lint": "bun exec expo lint",
    "build:gcloud": "bun run export:web && rm -rf web-build && mkdir -p web-build && cp -r web-build-temp/* web-build/ && cp custom-fonts.css web-build/ && rm -rf web-build-temp && node add-styles.js",
    "export:web": "bun exec expo export --platform web --output-dir web-build-temp && bun dlx expo-pwa",
    "deploy:gcloud": "bun run build:gcloud && gcloud app deploy --quiet",
    "build:vercel": "bun exec expo export --platform web --output-dir web-build",
    "vercel-build": "bun run build:vercel",
    "test": "jest --config testing/config/jest.config.js",
    "test:watch": "jest --config testing/config/jest.config.js --watch",
    "test:coverage": "jest --config testing/config/jest.config.js --coverage",
    "test:unit": "jest --config testing/config/jest.config.js --testPathPattern=testing/unit",
    "test:integration": "jest --config testing/config/jest.config.js --testPathPattern=testing/integration",
    "test:e2e": "jest --config testing/config/jest.config.js --testPathPattern=testing/e2e",
    "test:ci": "jest --config testing/config/jest.config.js --coverage --watchAll=false",
    "android": "bun exec expo run:android",
    "ios": "bun exec expo run:ios",
    "check-env": "bun scripts/check-env.js",
    "check-i18n": "bun scripts/check-duplicate-keys.js",
    "start:tunnel": "EXPO_NO_DOCTOR=1 bun exec expo start --tunnel --clear",
    "start:lan": "EXPO_NO_DOCTOR=1 bun exec expo start --lan --clear",
    "start:localhost": "EXPO_NO_DOCTOR=1 bun exec expo start --localhost --clear",
    "clear-cache": "EXPO_NO_DOCTOR=1 bun exec expo start --clear",
    "reset-cache": "bun run clear-cache",
    "deploy:vercel": "vercel --prod",
    "preview:vercel": "vercel"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.9.0",
    "cloudinary-react-native": "^1.2.1",
    "dotenv": "^17.0.0",
    "expo": "~54.0.0",
    "expo-blur": "^15.0.7",
    "expo-camera": "^17.0.8",
    "expo-constants": "^18.0.9",
    "expo-document-picker": "^14.0.7",
    "expo-font": "^14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "^8.0.8",
    "expo-localization": "^17.0.7",
    "expo-location": "^19.0.7",
    "expo-notifications": "^0.32.12",
    "expo-pwa": "^0.0.127",
    "expo-router": "~6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "^3.0.8",
    "expo-symbols": "^1.0.7",
    "expo-system-ui": "^6.0.7",
    "expo-web-browser": "^15.0.8",
    "firebase": "^12.4.0",
    "i18next": "^25.6.0",
    "jwt-decode": "^4.0.0",
    "lucide-react-native": "^0.475.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.0.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "serve": "^14.2.4",
    "socket.io-client": "^4.8.1",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^54.0.10",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "resolutions": {
    "react-native-worklets": "0.5.1"
  }
}
