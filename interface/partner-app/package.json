{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.0.0",
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start --clear --offline",
    "start": "npx expo start --clear --offline",
    "check:env": "tsx scripts/check-env.ts",
    "build:web": "bun exec expo export --platform web",
    "export:web": "bun exec expo export --platform web --output-dir web-build-temp && bun dlx expo-pwa",
    "build:gcloud": "bun run export:web && (if exist web-build rmdir /s /q web-build) && mkdir web-build && xcopy web-build-temp\\* web-build\\ /s /e /y && copy custom-fonts.css web-build\\ && node add-styles.js",
    "deploy:gcloud": "bun run build:gcloud && gcloud app deploy --quiet",
    "lint": "bun exec expo lint",
    "start:tunnel": "npx expo start --tunnel --clear --offline",
    "start:lan": "npx expo start --lan --clear --offline",
    "start:localhost": "npx expo start --localhost --clear --offline",
    "clear-cache": "npx expo start --clear --offline"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.6.0",
    "expo": "~54.0.0",
    "expo-blur": "^14.0.3",
    "expo-camera": "^16.0.18",
    "expo-constants": "^17.0.5",
    "expo-font": "^13.0.3",
    "expo-haptics": "^14.0.1",
    "expo-image-picker": "^15.1.0",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-pwa": "^0.0.127",
    "expo-router": "4.0.17",
    "expo-splash-screen": "^0.29.21",
    "expo-status-bar": "^2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "^4.0.7",
    "expo-web-browser": "^14.0.2",
    "lucide-react-native": "^0.475.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.23.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "serve": "^14.2.4",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.1",
    "typescript": "^5.3.3"
  }
}
